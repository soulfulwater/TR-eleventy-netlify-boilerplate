                        <a class="relative z-50 inline-block mr-4 py-2 whitespace-no-wrap" href="#">
                            <img src="{{ site.image }}" alt="" style="max-width: 100px;">
                        </a>
                        <div class="flex items-center">
                            <span class="text-white inline-block text-xs_xl" type="button" id="mobile-menu">
                                <i class="fas fa-bars"></i>
                            </span>
                            <a href="#"
