---

    <!-- start header -->
    <header class="lg:block absolute w-full  z-50 top-0">
        <div class="container px-8">
            <nav class="hidden lg:flex flex-wrap items-center justify-between px-2 navbar-expand-lg left-0 top"
                style="padding: 0;">
                <div class="mx-auto flex flex-wrap items-center justify-between w-full">
                    <div class="relative flex justify-between w-auto static block justify-start">
                        <a class="relative z-50 inline-block mr-4 py-2 whitespace-no-wrap" href="#" style="padding: 0;">
                            <img src="{{ site.image }}" alt="" style="height: 175px; object-fit: contain;">
                        </a>
                    </div>
                    <div class="flex-grow items-center flex">
                        <ul
                            class="flex flex-col md:flex-row list-none md:ml-auto space-y-4 md:space-y-0 md:space-x-6 lg:space-x-10  items-center pt-8 pb-4 md:pt-8 md:pb-4">
