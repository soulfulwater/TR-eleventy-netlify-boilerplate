                    class="text-xs_2xxl md:text-xs_4xl text-white font-bold  font-bold tracking-xxl_tight mb-6 leading-none">
                    {{ hero.headline }}
                </h2>
                <h4
                    class="two_half font-extralight text-xs_ssm md:text-xs_2xxl text-white leading-tight mb-6 tracking-two_half uppercase">
                    {{ hero.subheader }}
                </h4>
            </div>

        </div>

    </section>
    <!-- hero section -->


    <!-- about section -->
    <div class="bg-sec_primary pt-0 md:pt-32  lg:pt-0 pb-20 ">
        <div class="container px-6 xl:px-8 w-10/12" style="max-width: 1120px;">
            <div class="relative -mt-12 lg:-mt-0 md:grid grid-cols-1 md:grid-cols-3 md:gap-6 lg:gap-10">
                <div class="lg:-mt-12 mb-8">
